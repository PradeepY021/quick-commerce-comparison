{
  "name": "quick-commerce-comparison",
  "version": "1.0.0",
  "description": "Price comparison platform for quick-commerce platforms like Zepto, Swiggy, Blinkit, and BigBasket",
  "main": "server.js",
  "scripts": {
    "start": "node server-simple.js",
    "dev": "nodemon server-simple.js",
    "build": "npm run build:client",
    "build:client": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "build:render": "PUPPETEER_SKIP_DOWNLOAD=true npm install --production=false && cd client && npm install && npm run build"
  },
  "keywords": [
    "quick-commerce",
    "price-comparison",
    "e-commerce",
    "grocery",
    "delivery"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.1",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.11.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^4.6.10"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
